{
  "manifest_version": 3,
  "name": "Link Opener",
  "version": "1.0",
  "description": "Open a link in a new tab from the input field",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "hello.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html"
}